        $(function () {
            function tellogin_callback() {

            }
            $("#sbtn").click(function () {
                var tel = $("#username").val();
                var pwd = $("#password").val();
                var select = $("#select").val();
                if (tel == "") {
                    $("#note").text("请输入登录手机号");
                    return false;
                } else {
                    $("#note").text("");
                }
                if (pwd == "") {
                    $("#note").text("请输入登录密码");
                    return false;
                } else {
                    $("#note").text("");
                }
                $.post(
                    "http://qh.1caopan.com/index/api/login/",
                    {
                        account: tel,
                        password: pwd,
                        type: select,

                    },
                    function (_d) {
                        alert(_d['msg']);
                        if (_d['code'] == 1) {
                            localStorage.clear();
                            for (var key in _d["data"]) {
                                localStorage[key] = _d["data"][key];
                                localStorage.type = $("#select").val();;
                            }
                            window.location.href = "uc";
                        }
                    },
                    "JSON"
                );
