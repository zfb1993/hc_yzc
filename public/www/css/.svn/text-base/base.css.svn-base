@charset "utf-8";
/*让英文分行显示*/
*{word-wrap:break-word}
/*IE下a:hover 背景闪烁*/
*html{overflow:-moz-scrollbars-vertical;zoom:expression(function(ele){ele.style.zoom = "1";document.execCommand("BackgroundImageCache",false,true)}(this))}

em{font-style:normal;}
/*去除所有标签内的外和内边距*/
html,body,h1,h2,h3,h4,h5,h6,hr,p,iframe,dl,dt,dd,ul,ol,li,pre,form,button,input,textarea,th,td,fieldset{margin:0;padding:0}
body {
	font:14px "Microsoft YaHei","宋体",Arial,sans-serif; 
	background-color:#fff; 
	color:#555;
	min-width: 1200px;
}

/*网站的可视宽度*/
.w1200{
	width:1200px; 
	margin: 0 auto;
}

/*a标签的样式*/
a{color:#555;text-decoration:none;} 
a:hover{color:#ef8200} 
li{list-style: none;}
/*重置标签的一些样式*/
a{text-decoration:none;}
ul,ol,dl{list-style-type:none}

/*浮动*/
.fl{float:left;}
.fr{float:right;}
/*清除浮动*/
.clear{clear:both;}
.clearfix,.container{*zoom:1;} /*解决ie6/7下面的浮动塌陷问题*/
.clearfix:before,.clearfix:after,.container:before,.container:after {display:table; line-height:0; content:"";} /*display:table;解决其他下面的浮动塌陷问题,line-height是将添加的内容高度设为0；*/
.clearfix:after,.container:after{clear:both;}/*清楚浮动*/

/*去除图片的边框*/
img{border:none;}

/*文字*/
.tl{text-align:left;}
.tr{text-align:right;}
.tc{text-align:center;}

/*定位*/
.pr{position:relative;}
.pa{position:absolute;}

/*特殊文字颜色*/
.red{color:#F12727;}
.orange{color:#FFC000;}
.blue{color:#888DBD;}
.tf-hui{color:#b0b0b0 !important;}

.hidden{display:none;}

/*特殊字体大小*/
.f10{font-size:10px;}
.f12{font-size:12px;}
.f14{font-size:14px;}
.f16{font-size:16px;}
.f18{font-size:18px;}
.f20{font-size:20px;}

